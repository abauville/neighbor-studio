<%= render partial: "shared/navbar" %>

<div class="d-flex justify-content-between mx-5 add-studio">
  <h1 class="text-left mt-5 mb-5">Your Studios</h1>
  <div class="studio-info-btn py-2 px-2">
    <%= link_to "Add a Studio", new_studio_path, class: "btn-square py-5", id: "add-studio" %>
  </div>
</div>

<div class="h-100 row justify-content-center">
<% @studios.each do |studio| %>
  <%= render partial: "card", locals: {studio: studio}%>
<% end %>
</div>
